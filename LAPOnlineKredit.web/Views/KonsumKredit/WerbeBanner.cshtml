@model IEnumerable<LAPOnlineKredit.web.Models.ZusammenfassungModel>

<div id="content">

    <div class="row">
        <br /><br /><br />
        Unsere letzten 10 KreditAnträge!
        <br /><br />
    </div>
    @foreach (var item in Model)
    {
        <div class="row">
            <div class="col-lg-offset-1 col-lg-8">
                @item.Vorname.Substring(0, 1).@item.Nachname genießt jetzt @item.Betrag €

            </div>
        </div>
    }

</div>